{
  "name": "be-master",
  "version": "1.0.0",
  "description": "Express Service",
  "engines": {
    "node": ">=16.13.2"
  },
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && webpack",
    "start": "npm run build && node index.js",
    "start:dev": "npm run build && nodemon index.js --config nodemon.json",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "test": "jest --detectOpenHandles --forceExit",
    "sql:create": "sequelize-cli db:create",
    "sql:drop": "sequelize-cli db:drop",
    "sql:migrate": "sequelize-cli db:migrate",
    "sql:migrate:undo": "sequelize-cli db:migrate:undo",
    "sql:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "sql:seed:all": "sequelize-cli db:seed:all",
    "sql:seed:undo": "sequelize-cli db:seed:undo",
    "sql:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "sql:model": "node scripts/generate-models.js",
    "module-update": "ncu -u && npm update && npm install"
  },
  "keywords": [
    "nodejs",
    "expressjs"
  ],
  "author": "M.RIZKI I <muhammadrzki96@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/REXY4/codebase-master-microservices.git"
  },
  "bugs": {
    "url": "https://gitlab.com/emcgd/svc/be-master/issues"
  },
  "homepage": "https://github.com/REXY4/codebase-master-microservices/#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@faker-js/faker": "^7.2.0",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@joi/date": "^2.1.0",
    "amqplib": "^0.10.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "cron": "^2.0.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "email-templates": "^10.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "got": "^12.1.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "interval-promise": "^1.4.0",
    "jest": "^28.1.1",
    "joi": "^17.6.0",
    "joi-route-to-swagger": "^2.1.2",
    "jose": "^4.10.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mali": "^0.46.1",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.3.8",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "mssql": "^8.1.2",
    "mysql2": "^2.3.3",
