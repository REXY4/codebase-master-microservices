    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^14.0.1",
    "pre-commit": "^1.2.2",
    "supertest": "^6.2.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
